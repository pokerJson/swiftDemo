platform :ios, '10.0'
source 'https://github.com/CocoaPods/Specs.git'
# Comment this line if you're not using Swift and don't want to use dynamic frameworks
use_frameworks!

def app_pods
  pod 'Moya'
  pod 'SnapKit'
  pod 'Kingfisher'
  pod 'HandyJSON', '~> 5.0.2'
  pod 'MBProgressHUD'
  pod 'MJRefresh'
  pod 'Reachability'
  pod 'RealmSwift'
  pod 'Realm'
end


target 'EcologyPlanet' do

  app_pods

end
target 'EcologyPlanet Dev' do

  app_pods

end
